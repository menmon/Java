    private void buttontotalActionPerformed(java.awt.event.ActionEvent evt) {                                            
        // TODO add your handling code here:
            int total=0;
        
        if(cekboxSoto.isSelected()){
             if(textfieldSoto.getText().equals("")){
                 JOptionPane.showMessageDialog(null,"Jumlah Tidak Boleh Kosong");
            }
            else{
            int harga = Integer.parseInt(textfieldSoto.getText());
            total += harga*5000;
            }
        }
        if(cekboxRawon.isSelected()){
             if(textfieldRawon.getText().equals("")){
                 JOptionPane.showMessageDialog(null,"Jumlah Tidak Boleh Kosong");
            }
             else{
            int harga = Integer.parseInt(textfieldRawon.getText());
            total += harga*6000;
        }
        }
        if(cekboxBakso.isSelected()){
             if(textfieldBakso.getText().equals("")){
                 JOptionPane.showMessageDialog(null,"Jumlah Tidak Boleh Kosong");
            }
             else{
            int harga = Integer.parseInt(textfieldBakso.getText());
            total += harga*4000;
        }
        }
        if(cekboxGudeg.isSelected()){
             if(textfieldGudeg.getText().equals("")){
                 JOptionPane.showMessageDialog(null,"Jumlah Tidak Boleh Kosong");
            }
             else{
            int harga = Integer.parseInt(textfieldGudeg.getText());
            total += harga*8000;
        }
        }
        textfieldTotal.setText(" "+total);
                       
    }                                           

