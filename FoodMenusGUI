    private void buttonResetActionPerformed(java.awt.event.ActionEvent evt) {                                            
        // TODO add your handling code here:
         cekboxSoto.setSelected(false);
        cekboxRawon.setSelected(false);
        cekboxBakso.setSelected(false);
        cekboxGudeg.setSelected(false);
        textfieldSoto.setText("");
        textfieldRawon.setText("");
        textfieldBakso.setText("");
        textfieldGudeg.setText("");
        textfieldTotal.setText("");
    }                                           

